<IfModule mod_rewrite.c>
    RewriteEngine on
    # Exclude administrator URL from redirection
    RewriteCond %{REQUEST_URI} !^/MainCareer/administrator
    # Redirect all other requests to public directory
    RewriteCond %{REQUEST_URI} !^/MainCareer/public/
    RewriteRule ^(.*)$ /MainCareer/public/$1 [L]
</IfModule>